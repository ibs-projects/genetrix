<script>
    try {
        var ifram = document.getElementById('frame-archive');
        ifram.onload = function () {
            this.style = "width:100%;height:" + window.innerHeight + 'px';
            this.style.display = 'initial';
            document.getElementById('lf-frame-archive').style.display = "none";
            document.getElementById('lf-frame-archive').remove();
        //    document.getElementById('lf-' + this.id).remove();
        };
    } catch (e) {

    }
</script>