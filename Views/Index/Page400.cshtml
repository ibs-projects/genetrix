
@{
    ViewBag.Title = "404 Error";
    Layout = "~/Views/_Shared/_BlankLayout2.cshtml";
}
<style>
    #p-400{
        background-image: url('@Url.Content("~/assets/images/IU/aperçu.jpg")')
    }
</style>
<div class="my-5 pt-sm-5">
    <div id="p-400" class="container1">

        <div class="row hidden">
            <div class="col-md-12">
                <div class="text-center">
                    <div>
                        <div class="row justify-content-center">
                            <div class="col-sm-4">
